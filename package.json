{
  "name": "cushin-monorepo",
  "version": "1.1.1",
  "description": "Cushin code generators monorepo",
  "private": true,
  "type": "module",
  "license": "MIT",
  "author": "Le Viet Hoang",
  "repository": {
    "type": "git",
    "url": "https://github.com/cushin-org/codegen.git"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm -r dev",
    "typecheck": "pnpm -r typecheck",
    "test": "pnpm -r test",
    "lint": "pnpm -r lint",
    "prepublishOnly": "pnpm build",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "pnpm build && changeset publish",
    "prerelease": "pnpm build",
    "publish:dry": "changeset publish --dry-run"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@types/node": "^20.11.0",
    "eslint": "^8.56.0",
    "typescript": "^5.3.0"
  }
}
